<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PersonalManager.Pages.AddTaskPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="EntryTask" Placeholder="Fill Task"/>
            <Picker x:Name="PickerContact" TextColor="Black" />
            <Button x:Name="LoadPhoto" Text="Assing to People"/>
            <Button x:Name="Save" Text="Save task"/>
            <Button x:Name="Close" Text="Close" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>